{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "ecmaFeatures": {
        "jsx": true
    },
    "rules": {
        "quotes": [2, "single"],
        "eol-last": [0],
        "comma-dangle": [1,"never"],
        "no-dupe-keys":2,
        "no-duplicate-case":2,
        "no-empty":2,
        "no-func-assign":2,
        "no-unreachable":2,
        "use-isnan":2,
        "curly": [2,"all"],
        "no-eval":2,
        "no-extra-bind":2,
        "no-extra-label:":0,
        "no-fallthrough":2,
        "no-implied-eval":2,
        "no-loop-func":1,
        "no-lone-blocks":2,
        "no-self-assign":2,
        "no-self-compare":2,
        "no-sequences":2,
        "no-unmodified-loop-condition":2,
        "radix":0,
        "wrap-iife": [2,"any"],
        "no-undef":2,
        "no-undef-init":2,
        "no-unused-vars": [1, {"vars":"all","args":"none"}],
        "no-use-before-define":0,
        "array-bracket-spacing": [2,"never"],
        "block-spacing":[1,"never"],
        "brace-style": [2,"1tbs", {"allowSingleLine":true}],
        "camelcase":2,
        "newline-after-var":0,
        "newline-before-return":0,
        "no-const-assign":2,
        "prefer-const":0,
        "prefer-template":0,
        "constructor-super":0,
        "arrow-parens": 0,
        "arrow-body-style":1,
        "arrow-spacing":[1,{"before":true,"after":true}],
        "no-mixed-requires": [0],
        "no-underscore-dangle": [0],
        "no-console": 0,
        "no-debugger": 1,
        "no-var": 1,
        "semi": [1, "always"],
        "no-trailing-spaces": 0,
        "no-alert": 0,
        "no-new-symbol":2,
        "jsx-quotes": 1,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1
    },
    "plugins": [
        "eslint-plugin-react"
    ]
}